            </items>
          </Menu>
        </menus>
      </MenuBar>
      <HBox prefHeight="693.0" prefWidth="852.0">
         <children>
            <TabPane prefHeight="693.0" prefWidth="412.0">
               <tabs>
                  <Tab text="Add Transaction">
                     <content>
                        <VBox prefHeight="200.0" prefWidth="100.0">
                           <children>
                              <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Add Transaction">
                                 <font>
                                    <Font size="24.0" />
                                 </font>
                                 <VBox.margin>
                                    <Insets left="115.0" />
