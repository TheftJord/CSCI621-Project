                              </TextField>
                              <Button mnemonicParsing="false" onAction="#addTransaction" text="Submit">
                                 <VBox.margin>
                                    <Insets left="165.0" top="20.0" />
                                 </VBox.margin>
                              </Button>
                           </children>
                           <padding>
                              <Insets bottom="50.0" />
